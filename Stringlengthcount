import java.util.*;
public class Main{
    public static void main(String arg[]){
    Scanner sc=new Scanner(System.in);
    String s=sc.nextLine();
    System.out.print(stringlast(s));
}
public static int stringlast(String s){
    String []a=s.split(" ");
    int n=5;
    return(a[n].length());
}
}
