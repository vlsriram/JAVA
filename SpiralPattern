import java.util.*;
public class Pattern{
        public static void main(String[] args) {
            int n=4;
            int x=n+1;
            n=n*2;
            for (int row = 1; row < n ; row++) {
                for (int col = 1; col < n ; col++) {
                    int f = x - Math.min(Math.min(row, col), Math.min(n - row, n - col));
                    System.out.print(f + " ");
                }
                System.out.println();
  }

}
    }
