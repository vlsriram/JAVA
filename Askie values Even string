import java.util.*;
public class Evenascii {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s=sc.nextLine();
        int n=s.length();
        String str="";
        for(int i=0;i<n;i++)
        {
            char ch=s.charAt(i);
            int num=(int)ch;
            if(num%2==0)
            {
                str+=Character.toString(ch);
            }
            
        }
        System.out.print(str);
}
}
